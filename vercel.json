{
  "rewrites": [
    {
      "source": "/rdr-map/collector",
      "destination": "/rdr-map/collector/index.html"
    },
    {
      "source": "/rdr-map/main",
      "destination": "/rdr-map/main/index.html"
    },
    {
      "source": "/rdr-map/collector/(.*)",
      "destination": "/rdr-map/collector/$1"
    },
    {
      "source": "/rdr-map/main/(.*)",
      "destination": "/rdr-map/main/$1"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        }
      ]
    },
    {
      "source": "/rdr-map/(.*)\\.(css|js|png|jpg|jpeg|gif|svg|woff|woff2|ttf|eot|ico)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}
